    "version": "1.0.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/images/GOHER_ICON.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/GOHER.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "goher.android"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/GOHER.png",
        "backgroundColor": "#ffffff"
      },
      "package": "goher.android"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/GOHER_ICON.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-location",
